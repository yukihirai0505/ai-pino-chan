{
  "version": 2,
  "name": "ai-pino-chan",
  "alias": "ai-pino-chan.now.sh",
  "builds": [{ "src": "src/index.ts", "use": "@now/node-server" }],
  "routes": [{ "src": "/(.*)", "dest": "/src/index.ts" }],
  "env": {
    "LINE_CHANNEL_ACCESS_TOKEN": "@line-ai-pino-chan-channel-access-token",
    "LINE_CHANNEL_SECRET": "@line-ai-pino-chan-channel-secret",
    "A3RT_API_KEY": "@a3rt-api-key"
  }
}
